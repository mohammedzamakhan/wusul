<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <LangVersion>latest</LangVersion>
    <Nullable>enable</Nullable>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <PackageId>Wusul</PackageId>
    <Version>1.0.0</Version>
    <Authors>Wusul Team</Authors>
    <Company>Wusul</Company>
    <Description>Official C# SDK for Wusul - Digital Access Control Platform</Description>
    <PackageProjectUrl>https://github.com/mohammedzamakhan/wusul</PackageProjectUrl>
    <RepositoryUrl>https://github.com/mohammedzamakhan/wusul</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageTags>wusul;access-control;nfc;digital-wallet;apple-wallet;google-wallet;mena;sdk;api-client</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="13.0.3" />
    <PackageReference Include="System.Net.Http" Version="4.3.4" />
  </ItemGroup>

</Project>
